<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, width=device-width" />

    <link rel="stylesheet" href="/global.css" />
    <link rel="stylesheet" href="/contact-us.css" />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Inter:wght@200;300;400;500;600;700;800&display=swap"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Lexend:wght@600&display=swap"
    />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Kanit:wght@800&display=swap"
    />
  </head>
  <body>
    <div class="contact-us-torn">
      <main class="contact-us">
        <header class="header-jecati">
          <div class="header-jecati-child"></div>
          <div class="main-content">
            <div class="ellipse-parent">
              <div class="frame-child"></div>
              <img
                class="image-6-icon"
                loading="lazy"
                alt=""
                src="./public/image-6@2x.png"
              />
            </div>
            <div class="company-name">
              <h2 class="jecaticonstructionservices">
                <span>Jecati</span>
                <span class="constructionservices">ConstructionServices</span>
              </h2>
            </div>
          </div>
          <div class="navigation-menu">
            <div class="menu-options">
              <a class="home" href="/landing-page">Home</a>
            </div>
            <div class="menu-options1">
              <a class="about-us" href="/about-us">About us</a>
            </div>
            <div class="menu-options2">
              <a class="services">Services</a>
            </div>
            <div class="menu-options3">
              <a class="contact-us1" href="/contact-us">Contact Us</a>
            </div>
            <div class="project-title-parent">
              <div class="project-title">
                <a class="projects">Projects</a>
              </div>
              <button class="btn-inquire">
                <div class="btn-box"></div>
                <a class="inquire-now">INQUIRE NOW</a>
              </button>
            </div>
          </div>
        </header>
        <div class="healthiconsgeo-location-negat"></div>
        <section class="contact-us-inner">
          <div class="image-jecati-parent">
            <img
              class="image-jecati-icon"
              loading="lazy"
              alt=""
              src="./public/imagejecati@2x.png"
            />

            <div class="contact-title">
              <h1 class="contact-us2">Contact Us</h1>
              <div class="j-c-t-contact">
                <h1 class="jct-contact">JCT - Contact</h1>
              </div>
            </div>
          </div>
        </section>
        <section class="form-container-wrapper">
          <div class="form-container">
            <div class="input-fields">
              <div class="input-fields-child"></div>
              <div class="booking-form">
                <h3 class="booking-form1">- BOOKING FORM</h3>
                <h1 class="send-us-message">Send Us Message</h1>
              </div>
              <div class="input-name">
                <div class="input-field"></div>
                <input class="name" placeholder="Name*" type="text" />

                <img class="input-icon" alt="" src="./public/inputicon.svg" />
              </div>
              <div class="input-email">
                <div class="submit">Submit</div>
                <div class="input-field1"></div>
                <input
                  class="input-email-child"
                  placeholder="Email*"
                  type="text"
                />

                <img
                  class="input-icon1"
                  alt=""
                  src="./public/inputicon-1.svg"
                />
              </div>
              <div class="input-phone">
                <div class="input-field2"></div>
                <input class="phone" placeholder="Phone*" type="text" />

                <img
                  class="input-icon2"
                  alt=""
                  src="./public/inputicon-2.svg"
                />
              </div>
              <div class="input-address">
                <div class="input-field3"></div>
                <input
                  class="input-address-child"
                  placeholder="Address*"
                  type="text"
                />

                <img
                  class="input-icon3"
                  alt=""
                  src="./public/inputicon-3.svg"
                />
              </div>
              <div class="input-names">
                <textarea
                  class="textarea-message"
                  placeholder="Enter your question or message"
                  rows="{9}"
                  cols="{28}"
                ></textarea>
              </div>
              <div class="input-names1">
                <div class="btn-send" id="sendMessageButton">
                  <div class="btn-sendbox"></div>
                  <h1 class="send-message">
                    <p class="send-message1">Send Message</p>
                  </h1>
                </div>
              </div>
              <div class="social-media">
                <div class="social-media-child"></div>
                <div class="social-media-icons">
                  <div class="facebook-icon-container">
                    <img
                      class="icbaseline-facebook-icon"
                      loading="lazy"
                      alt=""
                      src="./public/icbaselinefacebook.svg"
                    />
                  </div>
                  <div class="facebook-rating">
                    <div class="rate-us-on-facebook-wrapper">
                      <h1 class="rate-us-on">Rate us on Facebook!</h1>
                    </div>
                    <div class="facebook-stars">
                      <img
                        class="material-symbolsstar-icon"
                        loading="lazy"
                        alt=""
                        src="./public/materialsymbolsstar.svg"
                      />

                      <img
                        class="material-symbolsstar-icon1"
                        loading="lazy"
                        alt=""
                        src="./public/materialsymbolsstar.svg"
                      />

                      <img
                        class="material-symbolsstar-icon2"
                        loading="lazy"
                        alt=""
                        src="./public/materialsymbolsstar-2.svg"
                      />

                      <img
                        class="material-symbolsstar-icon3"
                        loading="lazy"
                        alt=""
                        src="./public/materialsymbolsstar.svg"
                      />
                    </div>
                  </div>
                  <img
                    class="material-symbolsstar-icon4"
                    loading="lazy"
                    alt=""
                    src="./public/materialsymbolsstar.svg"
                  />
                </div>
              </div>
            </div>
            <div class="information-container-parent">
              <div class="information-container">
                <div class="information-container-child"></div>
                <div class="information-title">
                  <h3 class="information">- INFORMATION</h3>
                  <h1 class="get-in-touch-container">
                    <p class="get-in-touch">Get In Touch</p>
                  </h1>
                </div>
                <div class="address-container-parent">
                  <div class="address-container">
                    <div class="address-icon-container">
                      <div class="ellipse-group">
                        <div class="frame-item"></div>
                        <img
                          class="mdiaddress-marker-icon"
                          loading="lazy"
                          alt=""
                          src="./public/image/mdiaddressmarker.svg"
                        />
                      </div>
                    </div>
                    <div class="address-details">
                      <h2 class="our-address">Our address</h2>
                      <div class="lot-25-block">
                        Lot 25 Block 12 Green Valley Subdivision, San Nicholas
                        III 4102 Bacoor, Cavite
                      </div>
                    </div>
                  </div>
                  <div class="frame-inner"></div>
                </div>
                <div class="email-container-parent">
                  <div class="email-container">
                    <div class="email-icon-container">
                      <div class="email-icon-container-child"></div>
                      <img
                        class="clarityemail-line-icon"
                        loading="lazy"
                        alt=""
                        src="./public/clarityemailline.svg"
                      />
                    </div>
                  </div>
                  <div class="email-details">
                    <h2 class="mail-us-for-container">
                      <p class="mail-us-for">Mail us for information</p>
                    </h2>
                    <div class="plukgeoffreylorenztiugmailco">
                      plukgeoffreylorenztiu@gmail.com
                    </div>
                  </div>
                </div>
                <div class="information-container-item"></div>
                <div class="phone-container-parent">
                  <div class="phone-container">
                    <div class="phone-icon-container">
                      <div class="ellipse-container">
                        <div class="ellipse-div"></div>
                        <img
                          class="ggphone-icon"
                          loading="lazy"
                          alt=""
                          src="./public/image/ggphone.svg"
                        />
                      </div>
                    </div>
                    <div class="phone-details">
                      <h2 class="call-us-on">Call us on</h2>
                      <div class="details">
                        <div class="div">(046) 476-1578</div>
                        <div class="div1">+63 961 487 7434</div>
                        <div class="div2">+63 927 318 9633</div>
                      </div>
                    </div>
                  </div>
                  <div class="ellipse-parent1">
                    <div class="frame-child1"></div>
                    <div class="frame-child2"></div>
                  </div>
                </div>
                <div class="frame-child3"></div>
              </div>
              <div class="healthiconsgeo-location-negat1"></div>
            </div>
          </div>
        </section>
      </main>
      <footer class="footer-jecati">
        <div class="rectangle-parent">
          <div class="footer-jecati-child"></div>
          <div class="frame-child4"></div>
          <div class="follow-us">
            <div class="follow-us-child"></div>
            <h1 class="follow-us1">Follow Us</h1>
            <div class="social-media-accounts">
              <div class="facebook-icon-container1">
                <img
                  class="icbaseline-facebook-icon1"
                  loading="lazy"
                  alt=""
                  src="./public/image/icbaselinefacebook1.svg"
                />
              </div>
              <div class="facebook-icon-container2">
                <img
                  class="icbaseline-facebook-icon2"
                  loading="lazy"
                  alt=""
                  src="./public/icbaselineinstagram.svg"
                />
              </div>
              <div class="facebook-icon-container2">
                <img
                  class="icbaseline-facebook-icon2"
                  loading="lazy"
                  alt=""
                  src="./public/icbaselinelinkedin.svg"
                />
              </div>
            </div>
          </div>
        </div>
      </footer>
    </div>
    <script>
      document.getElementById("sendMessageButton").addEventListener("click", function () {
        alert("Message Sent!");
        // Additional logic for sending the message can be added here
      });
    </script>
  </body>
</html>
