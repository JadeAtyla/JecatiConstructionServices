                <div class="phone-number1">
                  <div class="blank-number">
                    <p class="p2">0928 1808 210</p>
                    <p class="p3">0918 9065 202</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="services-container-wrapper">
            <div class="services-container">
              <h2 class="our-services">Our Services</h2>
              <div class="services-list-container">
                <div class="services-list">
                  <div class="services-list-child"></div>
                  <div class="services-offered">
                    <div class="concrete-demolition">
                      <span>&gt;</span>
                      <span class="concrete-demolition1">
                        Concrete Demolition</span
                      >
                    </div>
                    <div class="stump-removal">
                      <span>&gt;</span>
                      <span class="stump-removal1"> Stump Removal</span>
                    </div>
                    <div class="construction-services1">
                      <span>&gt;</span>
                      <span class="construction-services2">
                        Construction Services</span
                      >
                    </div>
                    <div class="transport-equipment">
                      <span>&gt;</span>
                      <span class="transport-equipment1">
                        Transport Equipment</span
                      >
                    </div>
                    <div class="backhoe-on-hire-container">
                      <span>&gt;</span>
                      <span class="backhoe-on-hire"> Backhoe on Hire</span>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="working-hours-container-wrapper">
            <div class="working-hours-container">
              <h2 class="working-hours">Working Hours</h2>
              <div class="hours-list-container">
                <div class="hours-list">
                  <div class="hours-list-child"></div>
                  <div class="weekdays">
                    <div class="monday">Monday:</div>
                    <div class="tuesday">Tuesday:</div>
                    <div class="wednesday">Wednesday:</div>
                    <div class="thursday">Thursday:</div>
                    <div class="friday">Friday:</div>
                    <div class="saturday">Saturday:</div>
                    <div class="sunday">Sunday:</div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <footer class="footer-details">
        <div class="footer-background"></div>
        <div class="copyright-2024-container">
          <span>
            <span>Copyright © 2024 </span>
          </span>
          <span class="jecati-construction-services">
            <span> </span>
            <span class="jecati-construction-services1"
              >Jecati Construction Services
            </span>
          </span>
          <span>powered by TAKDA</span>
        </div>
        <div class="policy-container">
          <a class="privacy-policy">Privacy Policy</a>
        </div>
      </footer>
